    "cheerio": "^1.0.0-rc.2",
    "clang-format": "^1.0.55",
    "concurrently": "3.4.0",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.4",
    "fs": "0.0.1-security",
    "google-closure-compiler": "20171023.0.1",
    "html-webpack-plugin": "^2.30.1",
    "jasmine": "^2.5.3",
    "jasmine-spec-reporter": "^3.2.0",
    "jsdom": "^9.11.0",
    "jsdom-global": "^2.1.1",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-rollup-plugin": "0.2.4",
    "karma-spec-reporter": "0.0.26",
    "lite-server": "2.3.0",
    "livereload": "^0.6.3",
    "ngrx-store-freeze": "^0.2.0",
    "npm-run-all": "^4.1.2",
    "path": "^0.12.7",
    "protractor": "^5.1.2",
