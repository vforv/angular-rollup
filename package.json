{
  "name": "angular-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:prod": "PROD=true npm run build",
    "start": "npm-run-all --parallel dev liverelaod",
    "dev": "node config-compiling/node-rollup.js",
    "predev": "rm -rf dist && mkdir dist && cp node_modules/zone.js/dist/zone.min.js dist/zone.min.js && cp src/index.html dist/index.html",
    "liverelaod": "livereload 'dist/' -d",
    "build": "ngc -p ./tsconfig.json && rollup -c config-compiling/rollup.config.js",
    "postbuild": "cp node_modules/zone.js/dist/zone.min.js dist/zone.min.js && cp src/index.html dist/index.html",
    "prebuild": "rm -rf dist && rm -rf js",
    "serve": "http-server ./dist -g",
    "ngc": "ngc -p tsconfig.json",
    "test": "TEST=true karma start ./config-testing/karma.conf.js",
    "pretest": "rimraf dist",
    "debug": "source-map-explorer dist/client.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ngrx/store-devtools": "^4.1.1",
